@model RiftData.Presentation.ViewModels.Mobile.Shared.ViewModelBase
<!DOCTYPE html>
<html lang="en">
    <head>
        <title>@this.Model.MetaData.MetaTitle</title>
        <script type="text/javascript" src="http://apis.google.com/js/plusone.js"> </script>      
        <meta name="google" value="notranslate">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no" />
        <meta name="keywords" content="@this.Model.MetaData.MetaKeywords" />
        <meta name="descripton" content="@this.Model.MetaData.MetaDescription" />
        <link href="@Url.Content("~/Content/Mobile/mobile.css")" rel="stylesheet" type="text/css" />
        <link rel="stylesheet" href="http://code.jquery.com/mobile/1.0b3/jquery.mobile-1.0b3.min.css" />
        <script src="http://code.jquery.com/jquery-1.6.2.min.js"></script>
        <script src="http://code.jquery.com/mobile/1.0b3/jquery.mobile-1.0b3.min.js"></script>   
        <link href="@Url.Content("~/Scripts/Photoswipe/photoswipe.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/JQueryShare/jsShare.min.js")" type="text/javascript"></script>
        <link href="@Url.Content("~/Scripts/JQueryShare/jsShare.min.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/Photoswipe/lib/klass.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Photoswipe/code.photoswipe.jquery-2.1.3.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Mobile/Mobile.js")" type="text/javascript"></script>
        @this.Html.Partial("GoogleAnalytics")      
    </head>
    <body>
        <div data-role="page" id="RiftDataPage" data-add-back-btn="true">
            <div data-role="header">
                <h1>@Model.Header</h1>
            </div>
            <div data-role="content">
                @RenderSection("PageContent")
            </div>
        </div>
        @RenderSection("OtherPages", false)
    </body>

    @RenderSection("Scripts", false)
</html>